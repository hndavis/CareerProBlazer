<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<targets>
		<target name="logfile" xsi:type="File" fileName="CareerProUtil.log"  />
		<!-- <target name="errlog" xsi:type="File" fileName="CareerProUtilError.log" replaceFileContentsOnEachWrite="True"  /> -->
		
	</targets>

	<rules>
		<logger name="*" minlevel="Info" writeTo="logfile" />
		<!--<logger name="*" minlevel="Debug" writeTo="errlog" /> -->
	</rules>
</nlog>
